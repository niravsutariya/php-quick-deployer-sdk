{
    "name": "niravsutariya/quick-deployer-sdk",
    "description": "A simple to use PHP class to work with the Quick Deployer API",
    "keywords": [
      "laravel",
      "quick-deployer",
      "sdk",
      "api",
      "php",
      "quickdeployer"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "developer",
            "name": "Nirav Sutariya",
            "email": "sutariya_nirav@yahoo.com",
            "homepage": "https://www.niravsutariya.com/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/niravsutariya/php-quick-deployer-sdk.git"
    },
    "homepage": "https://github.com/niravsutariya/php-quick-deployer-sdk#readme",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "php-http/mock-client": "^1.5",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "QuickDeployer\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuickDeployer\\Tests\\": "tests/"
        }
    },
    "config": {
      "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "prefer-stable": true
}
